{
    "domain-name": "sample-domain",
    "restart-user-name": "%%APP-NAME%%-application-user",
    "eval-module": "%%APP-NAME%%-modules",
    "eval-root": "/",
    "conversion-enabled": false,
    "permission": [
      {
        "role-name": "rest-reader",
        "capability": "read"
      },
      {
        "role-name": "rest-writer",
        "capability": "update"
      }
    ]
}
